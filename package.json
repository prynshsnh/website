    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "concurrently": "^7.6.0",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "luxon": "^2.5.2",
    "markdown-it-footnote": "^3.0.3",
    "postcss": "^8.4.49",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.15"
